#!ipxe

echo v1
set server_ip 192.168.178.102:8086
set tftp_path /xubuntu-20.04.3-desktop-amd64/
kernel http://${server_ip}${tftp_path}/casper/vmlinuz.efi || read void
initrd http://${server_ip}${tftp_path}/casper/initrd.lz || read void
imgargs vmlinuz.efi initrd=initrd.lz root=/dev/nfs boot=casper netboot=nfs nfsroot=${server_ip}:${nfs_path} ip=dhcp splash quiet -- || read void
boot || read void
