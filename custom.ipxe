#!ipxe

set server_ip 192.168.178.102
set tftp_path /assets/xubuntu-20.04.3-desktop-amd64
kernel tftp://${server_ip}${tftp_path}/casper/vmlinuz.efi || read void
initrd tftp://${server_ip}${tftp_path}/casper/initrd.lz || read void
imgargs vmlinuz.efi initrd=initrd.lz root=/dev/nfs boot=casper netboot=nfs nfsroot=${server_ip}:${nfs_path} ip=dhcp splash quiet -- || read void
boot || read void
